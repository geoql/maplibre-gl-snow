{
  "name": "@geoql/maplibre-gl-snow",
  "version": "0.0.9",
  "private": false,
  "description": "A MapLibre GL JS custom layer for rendering falling snow particles via Three.js WebGPU compute shaders",
  "keywords": [
    "layer",
    "maplibre",
    "particles",
    "snow",
    "three.js",
    "weather",
    "webgpu"
  ],
  "homepage": "https://github.com/geoql/maplibre-gl-snow#readme",
  "bugs": {
    "url": "https://github.com/geoql/maplibre-gl-snow/issues"
  },
  "license": "MIT",
  "author": {
    "name": "Vinayak Kulkarni",
    "email": "inbox.vinayak@gmail.com",
    "url": "https://vinayakkulkarni.dev"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/geoql/maplibre-gl-snow.git"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.mjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.mts",
  "exports": {
    ".": {
      "types": "./dist/index.d.mts",
      "import": "./dist/index.mjs"
    }
  },
  "scripts": {
    "build": "tsdown",
    "dev": "tsdown --watch",
    "test": "echo 'test!'",
    "prepare": "is-ci || husky",
    "lint": "oxlint --deny-warnings",
    "lint:fix": "oxlint --fix",
    "format": "oxfmt --check",
    "format:fix": "oxfmt --write",
    "typecheck": "tsc --noEmit",
    "publish:npm": "npm publish --access public",
    "publish:jsr": "bunx jsr publish"
  },
  "devDependencies": {
    "@commitlint/cli": "^20.4.2",
    "@commitlint/config-conventional": "^20.4.2",
    "@commitlint/types": "^20.4.0",
    "@types/node": "^25.3.0",
    "husky": "^9.1.7",
    "is-ci": "^4.1.0",
    "lint-staged": "^16.2.7",
    "maplibre-gl": "^5.19.0",
    "oxfmt": "^0.35.0",
    "oxlint": "^1.50.0",
    "three": "^0.183.1",
    "tsdown": "^0.20.3",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "maplibre-gl": ">=3.0.0",
    "three": ">=0.183.0"
  },
  "engines": {
    "node": ">=24.0.0"
  },
  "packageManager": "bun@1.3.9"
}
